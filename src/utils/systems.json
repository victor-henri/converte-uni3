{
    "supplier": {
        "table": "fornecedores_origem",
        "destiny": "fornecedores_destino",
        "fields": {
            "codigo_origem": {
                "field_destiny": "codigo_destino",
                "transform": {
                }
            },
            "nome_origem": {
                "field_destiny": "nome_destino",
                "transform": {
                    "trim": true,
                    "clear": true
                }
            },
            "tipo_origem": {
                "field_destiny": "tipo_destino",
                "transform": {
                    "select": "FORNECEDOR",
                    "switch": {
                        "str_from": ["FORNECEDOR"],
                        "str_to": ["J"]
                    }
                }
            },
            "cnpj_origem": {
                "field_destiny": "cnpj_destino",
                "transform": {
                    "format": "CNPJ"
                }
            },
            "ie_origem": {
                "field_destiny": "ie_destino",
                "transform": {
                }
            },
            "observacao_origem": {
                "field_destiny": "observacao_destino",
                "transform": {
                    "trim": true,
                    "clear": true
                }
            },
            "endereco_origem": {
                "field_destiny": "endereco_destino",
                "transform": {
                    "trim": true,
                    "clear": true
                }
            },
            "numero_origem": {
                "field_destiny": "numero_destino",
                "transform": {
                }
            },
            "cidade_origem": {
                "field_destiny": "Cidade",
                "transform": {
                    "clear": true,
                    "upper": true,
                    "search": "CITY",
                    "copy": "Codigo_Cidade"
                }
            },
            "uf_origem": {
                "field_destiny": "UF",
                "transform": {
                }
            },
            "bairro_origem": {
                "field_destiny": "bairro_destino",
                "transform": {
                    "trim": true,
                    "clear": true
                }
            },
            "cep_origem": {
                "field_destiny": "cep_destino",
                "transform": {
                    "format": "CEP"
                }
            },
            "complemento_origem": {
                "field_destiny": "complemento_destino",
                "transform": {
                    "trim": true,
                    "clear": true
                }
            },
            "ativo_origem": {
                "field_destiny": "ativo_destino",
                "transform": {
                    "switch": {
                        "str_from": ["0", "1"],
                        "str_to": ["N", "S"]
                    }
                }
            },
            "email_origem": {
                "field_destiny": "email_destino",
                "transform": {
                    "trim": true
                }
            },
            "fone_origem": {
                "field_destiny": "fone_destino",
                "transform": {
                    "split": "DDD"
                }
            },
            "cel_origem": {
                "field_destiny": "cel_destino",
                "transform": {
                    "split": "DDD_cel"
                }
            }
        },
        "remove": {
            "column_1": "UF",
            "column_2": "Codigo_Cidade",
            "column_3": "Codigo_Cidade_IBGE"
        }
    }
}